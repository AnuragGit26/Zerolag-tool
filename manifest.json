{
	"manifest_version": 3,
	"name": "Sig/Prem Case Alerts",
	"version": "3.0.8",
	"description": "Triage Team New Sev1/Sev2 Cases Notifier for Sig - Sales/Service/Industry Cloud and Premier Weekend New Cases",
	"content_scripts": [
		{
			"matches": [
				"https://orgcs.my.salesforce.com/*"
			],
			"js": [
				"jsforce.js",
				"jquery.min.js",
				"bootstrap.min.js",
				"background.js"
			]
		}
	],
	"action": {
		"default_icon": "case.png"
	},
	"background": {
		"service_worker": "background.js"
	},
	"icons": {
		"16": "case.png",
		"48": "case.png",
		"128": "case.png"
	},
	"permissions": [
		"storage",
		"activeTab",
		"scripting",
		"tabs",
		"alarms",
		"cookies"
	],
	"host_permissions": [
		"https://orgcs.lightning.force.com/",
		"https://orgcs.my.salesforce.com/services/data/v59.0",
		"https://orgcs.my.salesforce.com/services/data/*",
		"https://orgcs.my.salesforce.com/*",
		"https://login.Salesforce.com/*",
		"https://test.Salesforce.com/*",
		"https://test.salesforce.com/",
		"https://login.salesforce.com/",
		"https://maxcdn.bootstrapcdn.com/*"
	]
}